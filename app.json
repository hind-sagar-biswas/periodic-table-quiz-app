{
  "expo": {
    "name": "periodic-table-quiz-app",
    "slug": "periodic-table-quiz-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/identity/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./src/assets/identity/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#111111"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/identity/adaptive-icon.png",
        "backgroundColor": "#111111"
      }
    },
    "web": {
      "favicon": "./src/assets/identity/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
